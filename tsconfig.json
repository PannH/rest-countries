{
   "compilerOptions": {
      "target": "ESNext",
      "module": "commonjs",
      "declaration": true,
      "rootDir": "src",
      "outDir": "dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "noImplicitAny": false,
      "strictNullChecks": false,
      "skipLibCheck": true,
      "resolveJsonModule": true
   }
}